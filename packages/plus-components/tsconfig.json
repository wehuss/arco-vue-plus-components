{
  "compilerOptions": {
    "outDir": "es",
    "target": "ES2015",
    "module": "ES2020",
    "moduleResolution": "node",
    "removeComments": true,
    "strict": true,
    "allowJs": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "jsx": "preserve",
    "paths": {
      "@/*": ["./*"],
      "scroll-into-view-if-needed":[
        "../../node_modules/scroll-into-view-if-needed/"
      ]
    },
  }
}
